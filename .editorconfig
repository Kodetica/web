# top-most editorconfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true

[*.{html, md}]
indent_style = space
indent_size = 2

[*.{js, css}]
indent_style = space
indent_size = 4

[*.toml]
indent_size = 2
indent_style = space
